<script>
	function LeftMenu(result) {
		var list = [];
		list.push("<li>" + result[0] + "</li>");
		for (i in result[1]) {
			if (result[1][i].current) {
				list.push("<li class='list current'>");
			} else {
				list.push("<li class='list'>");
			}
			list.push("<a href=" + result[1][i].url + ">");
			list.push("<div></div>");
			list.push("<span>" + result[1][i].name + "</span>");
			list.push("</a>");
			list.push("</li >");
		}
		document.getElementById('left-menu-list').innerHTML = list.join('');
	}
</script>

@model string

@if (!string.IsNullOrEmpty(Model))
{
    foreach (var item in Model.Split(','))
    {
        <div class='pull-left'>
            <a href="@item" target="_blank">
                <img src='@item' class='img-responsive img-thumbnail' style='width: 200px; height: 150px;' />
            </a>
        </div>
    }
}
